<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Titles -->
    <sys:String x:Key="NextRace">Next Grand Prix</sys:String>
    <sys:String x:Key="DriversStanding">Driver Standings</sys:String>
    <sys:String x:Key="GrandPrixName">Grand Prix</sys:String>
    <sys:String x:Key="Location">Location</sys:String>
    <sys:String x:Key="RaceTime">Race Time</sys:String>
    <sys:String x:Key="TrackMap">Track Map Preview</sys:String>

    <!-- Context menu -->
    <sys:String x:Key="RefreshNow">Refresh Now</sys:String>
    <sys:String x:Key="Language">Language</sys:String>
    <sys:String x:Key="CheckUpdates">Check Updates</sys:String>
    <sys:String x:Key="EnableAutostart">Enable Autostart</sys:String>
    <sys:String x:Key="Exit">Exit</sys:String>

    <!-- Optional future keys -->
    <sys:String x:Key="Practice1">Practice 1</sys:String>
    <sys:String x:Key="Practice2">Practice 2</sys:String>
    <sys:String x:Key="Practice3">Practice 3</sys:String>
    <sys:String x:Key="Qualifying">Qualifying</sys:String>
    <sys:String x:Key="Sprint">Sprint</sys:String>
    <sys:String x:Key="Settings">Settings</sys:String>

    <sys:String x:Key="WeekendSchedule">Weekend Schedule</sys:String>

</ResourceDictionary>

